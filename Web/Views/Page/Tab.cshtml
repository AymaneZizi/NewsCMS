@{
    ViewBag.Title = ViewBag.Title;
    Layout = null;
}
<script type="text/javascript">
    function tabResize(size) {
        $('#IFramePage_@ViewBag.q').height(size);
    }
    function resizeIframe(obj) {
        obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px';
    }
</script>
<div class="DPageContiner" style="width: 100%; height: 100%;min-height:400px; background-image: url('../../Images/facebook-loading.gif'); background-position: center; background-repeat: no-repeat; background-color: white">
    <iframe src='/Page/@ViewBag.q/false' onload='javascript:resizeIframe(this);' width='100%' frameborder='0' scrolling='no' id='IFramePage_@ViewBag.q'></iframe>
</div>
